{
  "manifest_version": 2,

  "name": "TabMailer",
  "description": "This extension saves your currently open tab and then emails you it at some later date",
  "version": "1.0",
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "https://localhost/*",
    "https://*/",
    "http://*/"
  ]
}